    "using SymPy\n",
    "\n",
    "M = Sym[\n",
    "     6  5 -2 -3\n",
    "    -3 -1  3  3\n",
    "     2  1 -2 -3\n",
    "    -1  1  5  5\n",
    "]\n",
    "P, J = M.jordan_form()\n",
    "display(P)\n",
    "display(J)\n",
    "P*J/P"
   ]
