     "data": {
      "text/html": [
       "$a x^{2} + b x + c$"
      ],
      "text/latex": [
       "a x^{2} + b x + c"
      ],
      "text/plain": [
       "a*x^2 + b*x + c"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "f = a*x^2 + b*x + c"
   ]
  },
  {
   "cell_type": "code",
