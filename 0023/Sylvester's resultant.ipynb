   "source": [
    "using AbstractAlgebra\n",
    "AbstractAlgebra.PrettyPrinting.set_html_as_latex(true)\n",
    "\n",
    "R, (a, b, c, l, m, n) = ZZ[\"a\", \"b\", \"c\", \"l\", \"m\", \"n\"]\n",
    "Rx, x = R[\"x\"]"
   ]
  },
