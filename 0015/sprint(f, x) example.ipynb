   "source": [
    "function show_Hello(io::IO, x)\n",
    "    print(io, \"Hello, \")\n",
    "    show(io, x)\n",
    "    print(io, '!')\n",
    "end\n",
    "show_Hello(x) = show_Hello(stdout, x)\n",
    "\n",
    "show_Hello(\"Julia\")"
   ]
  },
