   "source": [
    "using DynamicPolynomials\n",
    "@polyvar x[1:3]\n",
    "m = monomials(x, 0:3)\n",
    "mD = differentiate.(m, x[1]) * 1.0\n",
    "mD2 = convert.(Term{true, Float64}, differentiate.(m, x[1]))\n",
    "mD2 == mD"
   ]
